@model UnivercityCityFood.Models.UserSDN

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}


<section class="signUp">
    <div class="form">
        <div class="container">
            <h5>هل انتا متاكد من حذف هذا اليوم؟</h5>

            <div class="row">

                <div class="col-md-6 col-xl-4">
                    <div class="data-view">
                        <h5>الرقم القومى</h5>
                        <div class="field">
                            @Html.DisplayFor(model => model.UserID)
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-xl-4">
                    <div class="data-view">
                        <h5>نوع المستخدم</h5>
                        <div class="field">
                            @Html.DisplayFor(model => model.UserGroup)
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-xl-4">
                    <div class="data-view">
                        <h5>التأكيد</h5>
                        <div class="field">
                            @Html.DisplayFor(model => model.UserIDConfirm)
                        </div>
                    </div>
                </div>
            </div>
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="actions">
                    <input type="submit" value="حذف" class="btn btn-primary" />
                    @Html.ActionLink("العوده", "Index", "", new { @class = "btn btn-danger" })
                </div>
            }

        </div>
    </div>
</section>
