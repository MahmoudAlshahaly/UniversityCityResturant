                                <li><a href="~/Account/EditProfile">&nbsp;&nbsp;تعديل الحساب الخاص بك</a></li>

                            </ul>
                        </li>
                        <li class="dropdown" data-admin-menu>
                            <a href="#">
                                المدير العام
                                <span class="glyphicon glyphicon-chevron-down"></span>
                            </a>
                            <ul class="dropdown-menu" role="menu">
                                @*Place our menu items*@

                                <li class="dropdown-header">المدير العام</li>
                                <li class="divider" />
                                <li><a href="~/Faculties/Index">&nbsp;&nbsp;الكليات</a></li>
                                <li><a href="~/Days/Index">&nbsp;&nbsp;الايام</a></li>
                                <li><a href="~/GenerateBarCode/BarcodeWriter">&nbsp;&nbsp;الباركود</a></li>
